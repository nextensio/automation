package user.routing

default route_tag = ""

route_tag = rtag {
    some i
    app := data.hosts[i]
    input.host == app.host
    some n
    input.user.type == app.routeattrs[n].type[_]
    input.user.team[_] == app.routeattrs[n].team[_]
    input.user.dept[_] == app.routeattrs[n].dept[_]
    rtag := app.routeattrs[n].tag
}

